[run]
source = djangocms-installer
branch = True
omit =
    djangocms-installer/share/*
    djangocms-installer/__main__.py
    djangocms-installer/main.py
    djangocms-installer/config/urls.py
    djangocms-installer/config/settings.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.: